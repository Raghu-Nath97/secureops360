{
    "Version": "2012-10-17",
    "Statement": [
      { "Effect": "Allow", "Action": [
          "sts:GetCallerIdentity"
        ], "Resource": "*" },
  
      { "Effect": "Allow", "Action": [
          "ec2:*"
        ], "Resource": "*" },
  
      { "Effect": "Allow", "Action": [
          "sns:*"
        ], "Resource": "*" },
  
      { "Effect": "Allow", "Action": [
          "dynamodb:*"
        ], "Resource": "*" },
  
      { "Effect": "Allow", "Action": [
          "kinesis:*"
        ], "Resource": "*" },
  
      { "Effect": "Allow", "Action": [
          "apigateway:*"
        ], "Resource": "*" },
  
      { "Effect": "Allow", "Action": [
          "lambda:*",
          "logs:*"
        ], "Resource": "*" },
  
      { "Effect": "Allow", "Action": [
          "iam:CreateRole",
          "iam:DeleteRole",
          "iam:AttachRolePolicy",
          "iam:DetachRolePolicy",
          "iam:PutRolePolicy",
          "iam:DeleteRolePolicy",
          "iam:PassRole",
          "iam:GetRole",
          "iam:TagRole"
        ], "Resource": "*" },
  
      { "Effect": "Allow", "Action": [
          "s3:*"
        ], "Resource": "*" }
    ]
  }
  